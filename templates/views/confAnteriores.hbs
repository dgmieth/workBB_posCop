<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Configurações Anteriores</title>
    <link rel="stylesheet" href="/css/bootstrap/bootstrap.css">
    <link rel="stylesheet" href="/css/header.css">
    <link rel="stylesheet" href="/css/mainPage.css">
    <link rel="stylesheet" href="/css/footer.css">
    <link rel="icon" type="image/ico" href="/img/bbFavicon.ico">
    <link rel="stylesheet" href="/css/datatables/dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/select.dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/jquery.dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/fixedHeader.dataTables.css">
    <link rel="stylesheet" href="/css/datatables/dataTables.css">
</head>
<body>
    <header>{{> header}}</header>
<!-- ================================================================================================= -->
<!--ConfAnteriores Page - INICIO - DATATABLE -->
    <div id="configurationControls" class=".container-fluid">
        <div class=".container-fluid" style="margin-bottom:60px;padding-left:35px;padding-right:35px">
            <div class="row">
                <div class="col-lg-12">
                    <table id="relConfAnteriores" class="table table-striped table-hover" cellspacing="0" style="width:100%;color: black;">
                        <thead>
                            <tr>
                                <th class="d-table-cell d-sm-none d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 20px;">Ativa</th>
                                <th class="d-none d-sm-none d-md-none d-lg-none d-xl-table-cell" style="width: 30px;">Data Criação</th>
                                <th class="d-table-cell d-sm-none d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 60px;">Criada por</th>
                                <th class="d-none" style="width: 60px;">Público alvo</th>
                                <th class="d-none d-sm-table-cell d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 60px;">Sistema responsável</th>
                                <th class="d-none d-sm-table-cell d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 60px;">Prefixos Gecor</th>
                                <th class="d-none d-sm-table-cell d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 210px;">Situação vínculo da operação</th>
                                <th class="d-none d-sm-table-cell d-md-table-cell d-lg-table-cell  d-xl-table-cell" style="width: 60px;">Acordo registrado no portal</th>
                                <th class="d-none d-sm-table-cell d-md-table-cell d-lg-table-cell  d-xl-table-cell" style="width: 60px;">Quantidade</th>
                                <th class="d-none d-sm-none d-md-none d-lg-none d-xl-table-cell" style="width: 30px;">Data exclusão</th>
                                <th class="d-none d-sm-none d-md-none d-lg-none d-xl-table-cell" style="width: 60px;">Excluída por</th>
                            </tr>
                        </thead>             
                    </table>
                </div>
            </div>
        </div>
    </div>
<!--ConfAnteriores Page - FIM - DATATABLE -->
<!-- ================================================================================================= -->
    <footer>{{>footer}}</footer>
    
</body>
    <script src="/js/jquery/jquery.slim.min.js"></script>
    <script src="/js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="/js/popper/popper.min.js"></script>
    <script src="/js/datatables/jquery.dataTables.min.js"></script>
    <script src="/js/datatables/dataTables.select.min.js"></script>
    <script src="/js/confAnteriores/variablesAndFunctions.js"></script>
    <script src="/js/confAnteriores/loadDataToDataTable.js"></script>
    <script src="/js/datatables/dataTables.fixedHeader.js"></script>
    <script src="/js/datatables/ajax.moment.min.js"></script>
    <script src="/js/datatables/dataRender.datetime.js"></script>
</html>